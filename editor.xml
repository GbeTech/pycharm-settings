<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="DjangoTemplate" value="true" />
        <entry key="ECMAScript 6" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JSON" value="false" />
        <entry key="JavaScript" value="true" />
        <entry key="Jinja2" value="true" />
        <entry key="Python" value="true" />
        <entry key="TypeScript" value="false" />
        <entry key="XHTML" value="false" />
        <entry key="XML" value="false" />
        <entry key="yaml" value="false" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1000" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_TRAILING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_NOTIFICATION_AFTER_REFORMAT_CODE_ACTION" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
    <option name="ESCAPE_PASTED_TEXT" value="false" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}GOOD.*$">
      <option name="FOREGROUND" value="1c124" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}BAD.*$">
      <option name="FOREGROUND" value="c1170c" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}\[[A-Z\$].*$">
      <option name="FOREGROUND" value="498aca" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\[[*]{2}[a-zA-Z0-9\$\.\-\/\&gt;\(\)\, ]*\]">
      <option name="FOREGROUND" value="56a6f2" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="56a6f2" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\*)[^\*#\n]*">
      <option name="FOREGROUND" value="498aca" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\*\*)[^\*#\n]*">
      <option name="FOREGROUND" value="498aca" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(?&lt;=\*\*\*)[^\*#\n]*">
      <option name="FOREGROUND" value="56a6f2" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="56a6f2" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>