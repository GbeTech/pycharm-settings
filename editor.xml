<application>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
    <option name="ESCAPE_PASTED_TEXT" value="false" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}GOOD.*$">
      <option name="FOREGROUND" value="1c124" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="[ ]{1}BAD.*$">
      <option name="FOREGROUND" value="c1170c" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;![\w\d;\.,!?:-@#$%^&amp;()+])(?&lt;=\*  )[\w\d;\.,\(\)\s|\&lt;\&gt;']*$">
      <option name="FOREGROUND" value="498aca" />
      <option name="BACKGROUND" value="222222" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;![\w\d;\.,!?:-@#$%^&amp;()+])(?&lt;=\*{2}  )[\w\d;\.,\(\)\s|\&lt;\&gt;']*$">
      <option name="FOREGROUND" value="498aca" />
      <option name="BACKGROUND" value="111111" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="498aca" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;![\w\d;\.,!?:-@#$%^&amp;()+])(?&lt;=\*{3}  )[\w\d;\.,\(\)\s|\&lt;\&gt;']*$">
      <option name="FOREGROUND" value="56a6f2" />
      <option name="BACKGROUND" value="0" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="56a6f2" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
  </component>
</application>