<templateSet group="custom_surround">
  <template name="pr" value="print($SELECTION$)" description="print($selection$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="enum" value="enumerate($SELECTION$)" description="enumerate($SELECTION$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="for" value="for $I$ in $IT$:&#10;    $SELECTION$" description="surround with for" toReformat="false" toShortenFQNames="true">
    <variable name="I" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="IT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="tpe" value="type($SELECTION$)" description="type(selection" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="ra" value="range($SELECTION$)" description="range($selection$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="len" value="len($SELECTION$)$END$" description="len($selection$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="isin" value="if isinstance($VAR1$,$T$):&#10;    $END$" description="if isinstance($VAR1$,$T$):" toReformat="false" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="T" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="str" value="str($SELECTION$)" description="str($SELECTION$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="list" value="list($SELECTION$)" description="list($SELECTION$)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>