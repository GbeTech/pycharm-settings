<templateSet group="custom">
  <template name="forr" value="for $VAR1$ in range($VAR2$):&#10;    $END$ " description="" toReformat="true" toShortenFQNames="true">
    <variable name="VAR1" expression="index" defaultValue="i" alwaysStopAt="true" />
    <variable name="VAR2" expression="iterable" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="def" value="def $NAME$($P1$):&#10;    $END$" description="method" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="P1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="for" value="for $VAR1$ in $VAR2$:&#10;    $END$ " description="" toReformat="true" toShortenFQNames="true">
    <variable name="VAR1" expression="index" defaultValue="i" alwaysStopAt="true" />
    <variable name="VAR2" expression="iterable" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="iter" value="for $VAR$ in $ITERABLE$:&#10;    $END$" description="Iterate (for ... in ...)" toReformat="false" toShortenFQNames="true">
    <variable name="ITERABLE" expression="pyIterableVariable()" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR" expression="collectionElementName(ITERABLE)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="_" value="__$VAR$__$END$" description="dunder" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="try" value="try:&#10;    $END$&#10;except $E1$ as e:&#10;    print(&quot;Error: \n{}&quot;.format(str(e)))&#10;    raise" description="try except" toReformat="false" toShortenFQNames="true">
    <variable name="E1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="while" value="while $COND1$:&#10;    $END$" description="while" toReformat="false" toShortenFQNames="true">
    <variable name="COND1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="init" value="def __init__(self$P1$):&#10;    $END$" description="def __init__(self)" toReformat="false" toShortenFQNames="true">
    <variable name="P1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>