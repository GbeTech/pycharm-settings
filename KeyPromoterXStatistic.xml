<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="New...">
        <Item>
          <IdeaActionID>NewElement</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>New...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open...">
        <Item>
          <IdeaActionID>OpenFile</IdeaActionID>
          <ShortCut>'Ctrl+O'</ShortCut>
          <Description>Open...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Resume Program</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'index.html'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'F1'</ShortCut>
          <Description>Run 'index.html'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Local Changes as UML">
        <Item>
          <IdeaActionID>UML.ShowChanges</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+D'</ShortCut>
          <Description>Show Local Changes as UML</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Sort Lines">
        <Item>
          <IdeaActionID>linessorter.sort</IdeaActionID>
          <ShortCut>'Alt+Shift+L'</ShortCut>
          <Description>Sort Lines</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Sort code (A-Z)">
        <Item>
          <IdeaActionID>CodeBlockSorter.SortCaseInsensitiveAction</IdeaActionID>
          <ShortCut>'Alt+Shift+L'</ShortCut>
          <Description>Sort code (A-Z)</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'npm freeze'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5' or 'Shift+F1'</ShortCut>
          <Description>Stop 'npm freeze'</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'pip_freeze'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5' or 'Shift+F1'</ShortCut>
          <Description>Stop 'pip_freeze'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'sport-bingo'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5' or 'Shift+F1'</ShortCut>
          <Description>Stop 'sport-bingo'</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="VCS Operations Popup...">
        <Item>
          <IdeaActionID>Vcs.QuickListPopupAction</IdeaActionID>
          <ShortCut>'Alt+`'</ShortCut>
          <Description>VCS Operations Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Debug 'sport-bingo'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Debug 'sport-bingo'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F12' or 'Shift+F2' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>