<application>
  <component name="ActionMacroManager">
    <macro name="End-&gt;Period">
      <action id="EditorLineEnd" />
      <typing text-keycode="46:0">.</typing>
    </macro>
    <macro name="Go to recent file">
      <action id="RecentFiles" />
      <shortuct text="ENTER" />
    </macro>
    <macro name="ToTabs, Reformat and Save">
      <action id="ReformatCode" />
      <action id="SaveAll" />
    </macro>
    <macro name="words to list">
      <action id="SplitAction" />
      <action id="WrapAction" />
      <action id="WrapAction" />
      <action id="EditorBackSpace" />
      <typing text-keycode="91:0">[</typing>
      <shortuct text="TAB" />
      <action id="EditorBackSpace" />
      <action id="EditorBackSpace" />
      <typing text-keycode="93:0">]</typing>
      <action id="JoinAction" />
      <action id="EditorBackSpace" />
    </macro>
    <macro name="list to lines">
      <action id="SplitAction" />
      <shortuct text="ENTER" />
      <action id="Macro.ToTabs, Reformat and Save" />
      <action id="ConvertIndentsToTabs" />
      <action id="ReformatCode" />
      <action id="SaveAll" />
    </macro>
    <macro name="Debug Python Prompt" />
    <macro name="New Directory">
      <action id="NewElement" />
      <typing text-keycode="78:1;69:0;87:0;32:0;68:1;73:0;82:0;69:0;67:0;84:0;79:0;82:0;89:0">New&amp;#x20;Directory</typing>
      <shortuct text="ENTER" />
    </macro>
  </component>
</application>