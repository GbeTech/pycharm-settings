<application>
  <component name="XDebuggerSettings">
    <data-views>
      <sort-values>true</sort-values>
      <option name="valueLookupDelay" value="100" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
      <debugger id="oracle">
        <configuration>
          <option name="steppingPauseAtBegin" value="true" />
        </configuration>
      </debugger>
      <debugger id="python">
        <configuration>
          <option name="simplifiedView" value="false" />
          <option name="steppingFilters">
            <list>
              <PySteppingFilter>
                <option name="filter" value="**env**" />
              </PySteppingFilter>
            </list>
          </option>
          <option name="steppingFiltersEnabled" value="true" />
          <option name="watchReturnValues" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>