<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="valueLookupDelay" value="100" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="python">
        <configuration>
          <option name="librariesFilterEnabled" value="true" />
          <option name="watchReturnValues" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>